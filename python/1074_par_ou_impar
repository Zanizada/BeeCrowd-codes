N = int(input())
resultados = []

for _ in range(N):
    X = int(input())

    if X == 0:
        resultados.append("NULL")
    else:
        tipo = "EVEN" if X % 2 == 0 else "ODD"
        sinal = "POSITIVE" if X > 0 else "NEGATIVE"
        resultados.append(f"{tipo} {sinal}")

print("\n".join(resultados))
